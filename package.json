{
    "name": "frothers-webpage",
    "version": "1.0.0",
    "description": "Hugo + Typescript + Tina IO project for website",
    "main": "dist/main.js",
    "source": "assets/index.ts",
    "scripts": {
        "start": "run-p start:**",
        "start:hugo": "hugo -d ./dist -w",
        "start:tina": "yarn run tinacms dev",
        "start:webpack": "webpack serve --mode development --config webpack.dev.js --hot",
        "preview": "cross-env PORT=8080 run-p preview:**",
        "preview:hugo": "hugo -d ./dist -s site -w -D -F ",
        "preview:webpack": "webpack serve --mode development --config webpack.dev.js --host 0.0.0.0 --public",
        "build": "yarn run build:webpack && yarn run build:tina && yarn run build:hugo",
        "build:tina": "yarn tinacms build",
        "build:preview": "yarn run build:webpack && yarn run build:hugo:preview",
        "build:hugo": "hugo -d ./dist -b frothersfc.com",
        "build:hugo:preview": "yarn run build:hugo -- -D -F",
        "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.prod.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/frothers/Frothers-FC.git"
    },
    "author": "Chris Chester",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/frothers/Frothers-FC/issues"
    },
    "homepage": "https://github.com/frothers/Frothers-FC#readme",
    "devDependencies": {
        "@types/chart.js": "^2.9.29",
        "@types/chartjs-plugin-colorschemes": "^0.4.0",
        "@types/lodash": "4.14.168",
        "@types/styled-components": "^5.1.34",
        "assets-webpack-plugin": "^7.0.0",
        "axios": "^0.21.1",
        "chart.js": "^2.9.4",
        "chartjs-plugin-colorschemes": "^0.4.0",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^8.1.1",
        "cross-env": "7.0.3",
        "css-minimizer-webpack-plugin": "^3.4.1",
        "html-webpack-plugin": "5.3.1",
        "lodash": "4.17.21",
        "mini-css-extract-plugin": "1.5.0",
        "npm-run-all": "^4.1.5",
        "optimize-css-assets-webpack-plugin": "^6.0.1",
        "source-map-loader": "2.0.1",
        "ts-loader": "^9.0.0",
        "tslint": "^6.1.3",
        "typescript": "^4.2.4",
        "webpack": "^5.98.0",
        "webpack-cli": "^4.6.0",
        "webpack-dev-server": "^3.11.2",
        "webpack-merge": "^5.10.0"
    },
    "hugo-bin": {
        "buildTags": "extended"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@mui/icons-material": "^6.4.7",
        "@mui/material": "^6.4.7",
        "@mui/x-date-pickers": "^7.27.3",
        "@tinacms/cli": "^1.9.2",
        "@types/react-dom": "^18.2.18",
        "bootstrap": "^5.3.3",
        "html-to-image": "^1.11.13",
        "immutability-helper": "^3.1.1",
        "material-react-table": "^3.2.1",
        "process": "^0.11.10",
        "rdndmb-html5-to-touch": "8.0.3",
        "react": "^18.3.1",
        "react-bootstrap": "^2.10.9",
        "react-dnd": "^16.0.1",
        "react-dnd-html5-backend": "^16.0.1",
        "react-dnd-multi-backend": "8.0.3",
        "react-dnd-touch-backend": "^16.0.1",
        "react-dom": "^18.3.1",
        "recharts": "^2.15.1",
        "tinacms": "^2.7.2"
    }
}
